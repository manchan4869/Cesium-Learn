@import '../../style/main'

#toolbar {
	background-color: rgba(42, 42, 42, 1);
	opacity: 0.9;
	border-radius: 4px;
	padding: 11px;
	font-family: Microsoft YaHei;
	box-sizing: border-box;
}

.tool-bar{
    position: absolute;
    left: 10px;
    top: 10px;
    z-index: 10000;
}
.param-container{
    background-color: rgba(38, 38, 38, 0.75);
    padding: 20px 20px 10px 20px;
    color: #ffffff;
    border: 1px solid #526f82;
}